<h1>Categories</h1>
<table class="table table-striped  ">
  <thead class="thead-light table-dark">
    <tr>
      <th>Category Name</th>
    </tr>
  </thead>
  <tbody>
    <% @categories.each do |category| %>
      <tr>
        <td class=""><%= category.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br/>
<br/>


<%= link_to "Create New Category", new_admin_category_path , class: "btn btn-primary" %>



